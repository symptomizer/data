{
  "name": "data",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@glenstack/cf-workers-fetch-helpers": "1.3.0",
    "@glenstack/cf-workers-router": "1.0.1",
    "csv-parse": "4.15.1",
    "pdfjs-dist": "2.0.550",
    "react": "^16.13.1",
    "react-native": "^0.63.2",
    "realm": "^10.2.0"
  },
  "scripts": {
    "start": "wrangler dev",
    "build": "wrangler build",
    "lint": "eslint --ignore-path .gitignore --ext .js,.ts src/",
    "check-types": "tsc",
    "prettier": "prettier --ignore-path .gitignore \"**/*.+(js|ts|md)\"",
    "format": "npm run prettier -- --write",
    "check-format": "npm run prettier -- --list-different",
    "validate": "concurrently \"npm:check-types\" \"npm:check-format\" \"npm:lint\" \"npm:build\""
  },
  "devDependencies": {
    "@cloudflare/workers-types": "2.1.0",
    "@cloudflare/wrangler": "1.13.0",
    "@typescript-eslint/eslint-plugin": "4.14.1",
    "@typescript-eslint/parser": "4.14.1",
    "concurrently": "5.3.0",
    "eslint": "7.18.0",
    "eslint-config-prettier": "7.2.0",
    "husky": "4.3.8",
    "lint-staged": "10.5.3",
    "prettier": "2.2.1",
    "ts-loader": "8.0.14",
    "typescript": "4.1.3",
    "webpack": "^5.22.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run check-types && lint-staged"
    }
  },
  "lint-staged": {
    ".+(js|ts)": [
      "eslint"
    ],
    "**/*.+(js|ts|md)": [
      "prettier --write"
    ]
  }
}
